{
    "manifest_version": 3,
    "name": "AI Video Quizzer",
    "host_permissions": ["https://.youtube.com/*"],
    "description": "Ask Quizzer for help on any video. Let Quizzer test your knowledge, using AI! Learn and have fun!.",
    "background": {
        "service_worker": "background.js"
    },
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html",
        "default_title": "Open"
    },
    "permissions": ["storage", "tabs"]
}